{
    "name": "excel-ai-addon",
    "version": "1.0.0",
    "description": "AI assistant for Microsoft Excel",
    "main": "src/server/server.js",
    "proxy": "http://localhost:3001",
    "scripts": {
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "server": "node -r ts-node/register/transpile-only src/server/server.ts",
        "client": "react-scripts start",
        "build": "react-scripts build",
        "build:server": "tsc -p tsconfig.server.json",
        "test": "react-scripts test",
        "test:server": "npm run build:server && jest --config jest.config.js",
        "eject": "react-scripts eject",
        "dev": "node scripts/cleanup-logs.js && concurrently \"npm run server:dev\" \"npm run client:dev\"",
        "server:dev": "nodemon --watch src/server --ext ts --exec \"ts-node --project tsconfig.server.json --transpileOnly src/server/server.ts\"",
        "client:dev": "react-scripts start"
    },
    "dependencies": {
        "@microsoft/office-js": "^1.1.110",
        "axios": "^1.6.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.0.0",
        "openai": "^4.28.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "sanitize-html": "^2.13.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.10.0",
        "@types/office-js": "^1.0.79",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@types/sanitize-html": "^2.16.0",
        "ajv": "^8.17.1",
        "concurrently": "^8.2.2",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "ts-node": "^10.9.2",
        "typescript": "^4.9.5"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
