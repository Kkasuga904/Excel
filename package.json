{
  "name": "excel-ai-addon",
  "version": "1.0.0",
  "description": "Microsoft Excel用の完全自律型AIチャットアシスタント",
  "main": "src/server/server.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node -r ts-node/register/transpile-only src/server/server.ts",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "build:server": "tsc -p tsconfig.server.json",
    "test": "react-scripts test",
    "test:server": "jest --config jest.config.js",
    "eject": "react-scripts eject",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon --watch src/server --ext ts --exec \"node -r ts-node/register/transpile-only src/server/server.ts\"",
    "client:dev": "react-scripts start"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "openai": "^4.28.0",
    "axios": "^1.6.2",
    "@microsoft/office-js": "^1.1.110",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "sanitize-html": "^2.13.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "typescript": "^4.9.5",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "@types/jest": "^29.5.12",
    "@types/cors": "^2.8.19",
    "@types/sanitize-html": "^2.16.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
